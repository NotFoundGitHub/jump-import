{
	"name": "jump-import",
	"displayName": "jump-import",
	"description": "支持网易nej工程，vue工程，快速跳转到你import的文件里面去(无论你是 @还是 pool还是 ../都可以)，并支持跳转nei的mock数据data.json",
	"version": "0.0.5",
	"icon": "res/img/pic.jpg",
	"publisher": "notfoundgithub",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:vue",
		"onLanguage:scss",
		"onLanguage:css",
		"onLanguage:markdown",
		"onLanguage:json",
		"onLanguage:FreeMarker",
		"onLanguage:html"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "jump",
			"properties": {
			  "jump.alias": {
				"default": null,
				"description": "alias"
			  }
			}
		  }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"publish": "vsce package && vsce publish patch",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
